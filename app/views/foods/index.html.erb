<h1> Search Results: <%= @foods.count %></h1>
<% @foods.each do |food| %>
  <ul class='food'>
    <p class='food_code'>
      <%= food.food_code %>
    </p>
    <p class='description'>
      <%= food.description %>
    </p>
    <p class='brand_owner'>
      <%= food.brand_owner %>
    </p>
    <p class='ingredients'>
      <%= food.ingredients %>
    </p>
  </ul>
<% end %>
